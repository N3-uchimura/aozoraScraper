{
  "name": "aozorascrape",
  "productName": "aozorascrape",
  "version": "0.0.1",
  "description": "Electron application for scraping",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && chcp 65001",
    "package-win": "tsc && chcp 65001 && electron-packager . --asar=true --overwrite --platform=win32 --arch=ia32 --prune=true --icon=./assets/google.ico --out=release-builds --electronVersion=28.2.0",
    "start": "tsc && chcp 65001 && electron ."
  },
  "keywords": [],
  "author": "ebs-uchimura <uchimura@ebisu-do.jp>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.8.9",
    "electron-log": "^5.1.0",
    "path": "^0.12.7",
    "puppeteer-core": "^22.7.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "electron": "^29.1.1",
    "electron-packager": "^17.1.2"
  }
}